<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Converters1="using:Tutorial.Core.Converters"
    xmlns:Converters2="using:Tutorial.UI.WinRT.Converters" >

    <Converters1:StringReverseValueConverter x:Key="StringReverseValueConverter"/>
    <Converters1:StringLengthValueConverter x:Key="StringLengthValueConverter"/>
    <Converters1:FloatConverter x:Key="FloatValueConverter" />
    <Converters1:IntConverter x:Key="IntValueConverter" />
    <Converters2:TypeToNameConverter x:Key="TypeToNameConverter"/>
    <Converters2:InvertedConverter x:Key="InvertedConverter" />
</ResourceDictionary>
